{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.fireflys": {
			"initial_state": "dark",
			"states": {
				"light": {
					"on_entry": [
						"@s varent_0",
						"/scriptevent zombie:firefly2"
					],
					"transitions": [
						{
							"dark": "query.variant==1"
						}
					]
				},
				"dark": {
					"on_entry": [
						"@s varent_1"
					],
					"transitions": [
						{
							"light": "query.variant==0"
						}
					]
				}
			}
		}
	}
}